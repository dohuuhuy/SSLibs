    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-standard": "^5.0.0",
    "gh-pages": "^2.2.0",
    "microbundle-crl": "^0.13.10",
    "next-compose-plugins": "2.2.1",
    "next-images": "1.8.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "1.56.2",
    "typescript": "4.9.4",
    "bootstrap-icons": "1.10.2",
    "next": "^12"
